@model List<SigortaWeb.Models.BranchGroup>

<div class="form-group m-form__group ">
    <div class="form-group m-form__group">
        
            <label>
                Ana Branş
                <small style="color:red" id="notificationbg" onclick="deselectBranch()"></small>
            </label>
        
            <br />
            <select  multiple="multiple" id="branchGroupSelector">
                @foreach (var item in Model)
                {
                    <option value="@item.id"> @item.name</option>
                }
            </select>
    </div>
</div>


